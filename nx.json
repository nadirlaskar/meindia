{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build"
        ]
      }
    }
  },
  "targetDefaults": {
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "start-dev": {
      "dependsOn": [
        "^start-dev"
      ]
    },
    "build": {
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/*.js"
      ]
    },
    "docker:build": {
      "dependsOn": [
        "^docker:base:build"
      ]
    }
  }
}